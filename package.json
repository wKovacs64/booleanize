{
  "name": "@wkovacs64/booleanize",
  "version": "0.0.0-semantically-released",
  "description": "Small utility to coerce a value to its boolean equivalent",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/booleanize.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/booleanize/issues"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "build": "tsup ./src/index.ts --format cjs,esm --dts --sourcemap --clean",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm-run-all husky:install build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@types/jest": "27.4.0",
    "@wkovacs64/prettier-config": "3.0.0",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.6.0",
    "eslint-plugin-wkovacs64": "13.8.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.1.6",
    "npm-run-all": "4.1.5",
    "semantic-release": "18.0.1",
    "tsup": "5.11.11",
    "typescript": "4.5.4"
  }
}
