{
  "name": "@wkovacs64/booleanize",
  "version": "0.0.0-semantically-released",
  "description": "Small utility to coerce a value to its boolean equivalent",
  "main": "dist/index.js",
  "module": "dist/booleanize.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/test"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/booleanize.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/booleanize/issues"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "husky:install": "husky install",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "npm-run-all husky:install build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@wkovacs64/prettier-config": "3.0.0",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "npm-run-all": "4.1.5",
    "semantic-release": "17.4.7",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.4.2"
  }
}
