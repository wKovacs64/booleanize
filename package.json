{
  "name": "@wkovacs64/booleanize",
  "version": "0.0.0-semantically-released",
  "description": "Small utility to coerce a value to its boolean equivalent",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/booleanize.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/booleanize/issues"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write .",
    "build": "tsup ./src/index.ts --format cjs,esm --dts --sourcemap --clean",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm-run-all husky:install build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@types/node": "17.0.8",
    "@types/ws": "8.5.3",
    "@wkovacs64/prettier-config": "3.0.1",
    "c8": "7.11.2",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.13.0",
    "eslint-plugin-wkovacs64": "13.13.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2",
    "tsup": "5.12.6",
    "typescript": "4.6.3",
    "vitest": "0.9.3"
  }
}
