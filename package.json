{
  "name": "@wkovacs64/booleanize",
  "version": "0.0.0-semantically-released",
  "description": "Small utility to coerce a value to its boolean equivalent",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/booleanize.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/booleanize/issues"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts --sourcemap --clean",
    "check-types": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "lint": "eslint .",
    "husky:install": "husky install",
    "prepare": "run-s husky:install build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@types/node": "17.0.8",
    "@types/ws": "8.5.3",
    "@vitest/coverage-c8": "0.23.2",
    "@wkovacs64/prettier-config": "3.0.2",
    "c8": "7.12.0",
    "codecov": "3.8.3",
    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.23.1",
    "eslint-plugin-wkovacs64": "13.16.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "tsup": "6.2.3",
    "typescript": "4.8.3",
    "vitest": "0.23.4"
  }
}
