{
  "extends": [
    "config:js-lib",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)"
  ],
  "commitMessageTopic": "{{depName}}",
  "automergeType": "branch",
  "automerge": true,
  "major": {
    "automerge": false
  },
  "packageRules": [
    {
      "matchPackageNames": ["eslint"],
      "matchUpdateTypes": ["major"],
      "enabled": false
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}
